@model Contoso.Apps.Movies.Web.Models.UserAnalyticsModel
@{
    ViewBag.Title = "User Analytics";
}

    <section class="store">

        <div class="container">
            <h2>Personalized recommendations</h2>
        </div>

        <div class="container">
            <div class="row">
                <div class="wrapper clearfix">
